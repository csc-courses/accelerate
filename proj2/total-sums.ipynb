    "def total_sum_seq(a):\n",
    "    s = 0\n",
    "    for i in range(len(a)):\n",
    "        s += a[i]\n",
    "    return s\n",
    "\n",
    "    \n",
    "def fill_array(a):\n",
    "    for i in range(len(a)):\n",
    "        a[i] = i\n",
    "    return a\n",
    "\n",
    "SIZE = 1024\n",
    "a = [0]*SIZE\n",
    "fill_array(a)\n",
    "print(total_sum_seq(a))\n",
    "print(1024*1023//2)\n",
    "\n"
   ]
